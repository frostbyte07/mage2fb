{
  "name": "chrisqueen/mage2fb",
  "description": "Magento 2 module that will integrate a Magento store with FaceBook's Business manager features such as the FB pixel and product catalog for dynamic ads.",
  "type": "magento2-module",
  "homepage": "https://bitbucket.org/chrisqueen/mage2fb",
  "license": [
    "GPL-3.0"
  ],
  "support": {
    "email": "christopher.queen@gmail.com",
    "issues": "https://bitbucket.org/chrisqueen/mage2fb/issues"
  },
  "authors": [
    {
      "name": "Christopher Queen",
      "email": "christopher.queen@gmail.com"
    }
  ],
  "require": {
    "php": "~5.5.0|~5.6.0|~7.0.0",
    "magento/framework": "~100.0.4",
    "magento/magento-composer-installer": "*"
  },
  "require-dev": {
    "satooshi/php-coveralls": "dev-master",
    "phpunit/phpunit": "5.*",
    "phpunit/phpcov": "3.*",
    "ext-xdebug": ">=2.4.0",
    "phpunit/php-invoker": "~1.1",
    "browserstack/phpunit-browserstack": "dev-master"
  },
  "repositories": [
    {
      "type": "vcs",
      "url":  "https://github.com/browserstack/phpunit-browserstack.git"
    }
  ],
  "extra": {
    "map": [
      [
        "src",
        "ChrisQueen/Mage2fb"
      ]
    ]
  }
}